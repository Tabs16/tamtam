msgid "Triangle"
msgstr "Üçgen"

#: SynthLab/SynthLabConstants.py:97
msgid "Bi-Square"
msgstr "İki-kare"

#: SynthLab/SynthLabConstants.py:97
msgid "Uni-Square"
msgstr "Tek-kare"

#: SynthLab/SynthLabConstants.py:97 SynthLab/SynthLabConstants.py:177
